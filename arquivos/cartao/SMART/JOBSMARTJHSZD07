/*ROUTE PRINT JES2PRD1
//*DIARIA JV1. DATA:DATA_MOVIMENTO
//*DIARIA JV1. PERIODO:PERIODO_MOVIMENTO
//*DIARIA JV1. ANO MES:1905
//*DIARIA JV1. DIA:14
//*DIARIA JV1. DATA ANTERIOR:190513
//JOBLIB   DD  DISP=SHR,DSN=DSNC10.DB2M.SDSNLOAD
//         DD  DISP=SHR,DSN=HOM.V01.LOAD
//*
//**********************************************************************
//E001DELE EXEC PGM=IEFBR14
//SYSOUT   DD SYSOUT=*
//SYSPRINT DD SYSOUT=*
//SYSUT011 DD DISP=(MOD,DELETE),UNIT=3390,SPACE=(TRK,(5,5)),
//         DSN=HOM.SZ.EMP.MZ.BBM2.IEMPDAL1.DDATA_MOVIMENTO
//*
//E001GET  EXEC PGM=FTP,REGION=0M,PARM='10.100.2.144'
//STEPLIB  DD DSN=TCPIP.SEZALOAD,DISP=SHR
//SYSTCPD  DD DSN=HOM1.TCPPARMS(TCPDATA),DISP=SHR
//SYSFTPD  DD DSN=HOM1.TCPPARMS(FTPSDATA),DISP=SHR
//SYSMDUMP DD SYSOUT=Z
//SYSOUT   DD SYSOUT=*
//SYSPRINT DD SYSOUT=*
//INPUT    DD *
USERPROD USRPRD
LOCSITE TRACKS
LOCSITE PRIMARY=5000
LOCSITE SECONDARY=4000
LOCSITE LRECL=405 BLKSIZE=0 RECFM=FB
CD /UPLOAD/JV1/JDSZD07/ENTRADA
GET CNT.EMP.MZ.BBM2.IEMPDAL1.DDATA_MOVIMENTO   +
   'HOM.SZ.EMP.MZ.BBM2.IEMPDAL1.DDATA_MOVIMENTO'
QUIT
//**       =======================================================
//**       PROCESSAMENTO REPASSES SIFEC
//**       =======================================================
//E002DELE EXEC PGM=IEFBR14
//SYSOUT   DD SYSOUT=*
//SYSPRINT DD SYSOUT=*
//SYSUT021 DD DISP=(MOD,DELETE),UNIT=3390,SPACE=(TRK,(5,5)),
