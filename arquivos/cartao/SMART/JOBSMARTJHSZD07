//*
//E003SORT EXEC PGM=SORT,REGION=0K
//SYSPRINT DD SYSOUT=*
//SORTMSG  DD SYSOUT=*
//SYSOUT   DD SYSOUT=*
//SYSPRINT DD SYSOUT=*
//SORTIN   DD DSN=HOM.SZ.EMP.MZ.BBM2.IEMPDAL1.DDATA_MOVIMENTO,DISP=SHR
//SORTOUT1 DD DSN=HOM.SZ.DDATA_MOVIMENTO.IEMPDAL1.S01.SORT,
//         DISP=(,CATLG),LRECL=405,RECFM=FB,
//         UNIT=3390,SPACE=(TRK,(400,50),RLSE)
//SORTOUT2 DD DSN=HOM.SZ.DDATA_MOVIMENTO.IEMPDAL1.S02.SORT,
//         DISP=(,CATLG),LRECL=405,RECFM=FB,
//         UNIT=3390,SPACE=(TRK,(400,50),RLSE)
//SORTWK01 DD UNIT=3390,SPACE=(CYL,(50,50))
//SYSIN    DD *
  SORT FIELDS=(1,1,CH,A,2,18,CH,A)

* GRAVA ARQUIVO DE SAIDA AVERBACAO E HEADER
  OUTFIL FNAMES=SORTOUT1,REMOVECC,INCLUDE=(1,1,SS,EQ,C'0,1')

* GRAVA ARQUIVO DE SAIDA CANCELAMENTOS
  OUTFIL FNAMES=SORTOUT2,REMOVECC,INCLUDE=(1,1,SS,EQ,C'2')
//**       ==========================================================
//E003DELE EXEC PGM=IEFBR14
//SYSOUT   DD SYSOUT=*
//SYSPRINT DD SYSOUT=*
//SYSUT031 DD DISP=(MOD,DELETE),UNIT=3390,SPACE=(TRK,(5,5)),
//         DSN=HOM.SZ.DDATA_MOVIMENTO.IEMPDAL1.DUPLICA.SORT
//SYSUT032 DD DISP=(MOD,DELETE),UNIT=3390,SPACE=(TRK,(5,5)),
//         DSN=HOM.SZ.DDATA_MOVIMENTO.IEMPDAL1.S03.SORT
//SYSUT042 DD DISP=(MOD,DELETE),UNIT=3390,SPACE=(TRK,(5,5)),
//         DSN=HOM.SZ.DDATA_MOVIMENTO.IEMPDAL1.S04.SORT
//*
//E004SORT EXEC PGM=SORT,REGION=0K
//SYSPRINT DD SYSOUT=*
//SORTMSG  DD SYSOUT=*
//SYSOUT   DD SYSOUT=*
//SYSPRINT DD SYSOUT=*
//AVERBA   DD DSN=HOM.SZ.DDATA_MOVIMENTO.IEMPDAL1.S01.SORT,DISP=SHR
//CANCEL   DD DSN=HOM.SZ.DDATA_MOVIMENTO.IEMPDAL1.S02.SORT,DISP=SHR
//*
//DUPLICA  DD DSN=HOM.SZ.DDATA_MOVIMENTO.IEMPDAL1.DUPLICA.SORT,
//         DISP=(,CATLG),LRECL=405,RECFM=FB,
//         UNIT=3390,SPACE=(TRK,(400,50),RLSE)
//SORTOUT3 DD DSN=HOM.SZ.DDATA_MOVIMENTO.IEMPDAL1.S03.SORT,
//         DISP=(,CATLG),LRECL=405,RECFM=FB,
//         UNIT=3390,SPACE=(TRK,(400,50),RLSE)
//SORTOUT4 DD DSN=HOM.SZ.DDATA_MOVIMENTO.IEMPDAL1.S04.SORT,
//         DISP=(,CATLG),LRECL=405,RECFM=FB,
//         UNIT=3390,SPACE=(TRK,(400,50),RLSE)
//SORTWK02 DD UNIT=3390,SPACE=(CYL,(50,50))
//SYSIN    DD *
  SORT FIELDS=(1,1,CH,A,2,18,CH,A)
  JOINKEYS F1=AVERBA,FIELDS=(01,19,A)
  JOINKEYS F2=CANCEL,FIELDS=(01,19,A)
  JOIN UNPAIRED,F1,F2
  REFORMAT FIELDS=(?,F1:1,405,F2:1,405)
  OUTFIL FNAMES=DUPLICA,INCLUDE=(1,1,CH,EQ,C'B'),
               OUTREC=(1:2,405,/,1:406,405)
  OUTFIL FNAMES=SORTOUT3,INCLUDE=(1,1,CH,EQ,C'1'),BUILD=(1:2,405)
  OUTFIL FNAMES=SORTOUT4,INCLUDE=(1,1,CH,EQ,C'2'),BUILD=(1:407,405)
//******************************************************************
//E004DELE EXEC PGM=IEFBR14
//SYSOUT   DD SYSOUT=*
//SYSPRINT DD SYSOUT=*
//SYSUT041 DD DISP=(MOD,DELETE),UNIT=3390,SPACE=(TRK,(5,5)),
//         DSN=HOM.SZ.DDATA_MOVIMENTO.IEMPDAL1.S05.SORT
